#!/usr/bin/env python

import sys

if len(sys.argv) >= 2:
    f = open(sys.argv[1])
else:
    f = sys.stdin

countmap = {}
for line in f:
    if line in countmap:
        countmap[line] += 1
    else:
        countmap[line] = 1

for line in countmap:
    print "Line: %s Count: %s" % (line, countmap[line])

